{
  "plugins": [
      "prettier"
  ],
  "extends": [
      "react-app",
      "next/core-web-vitals",
      "prettier"
  ],
  "rules": {
      "prettier/prettier": [
          "warn",
          {
              "printWidth": 200,
              "semi": true,
              "jsxSingleQuote": true,
              "singleQuote": true,
              "useTabs": true,
              "arrowParens": "avoid"
          }
      ]
  }
}